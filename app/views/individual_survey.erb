survey number <%= @survey.id %> by <%= @survey.user.username %>

<h3> <%= @survey.title %> </h3>

<% @survey.questions.each do |question| %>
  <h5><%= question.title %><br></h5>
  <% question.possible_choices.each do |choice|%>
    <input type="radio" name="<%= question.id %>"><%= choice.content %>
    <br>


  <% end %>
<% end %>
