<div data-survey_num="<%= @survey.id %>" id="main">
	<div class="survey_num"><%= @survey.id %></div>
	<% @survey.questions.each_with_index do |question,index| %>

	<section>  
		<h5 data-q_num="<%= index+1 %>"><%="##{index+1} #{question.title }" %></h5>
		<% question.possible_choices.each do |choice| %>
		<p> <%= choice.content %></p>
		<% end %>
	</section>
	<% end %>
