

<h1><%=@user.username%></h1>
<% @surveys.each do |survey| %>
<p><a href="/surveys/<%=survey.id%>"><%= survey.title%></a></p>

<form action="/survey/<%=survey.id%>/delete" method= "POST">
  <input type="hidden" name="_method" value="Delete">
  <input type="submit" value="Delete Survey">
</form>
<a href="/survey/<%=survey.id%>/update">Update</a>
<%end%>
<p></p>
<p><a href='/surveys'>Back to ALL SURVEY</a></p>


